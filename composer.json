{
    "name": "exiliensoft/contact",
    "description": "send the email",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {},
    "autoload": {
        "psr-4": {
            "Exiliensoft\\Contact\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Exiliensoft\\Contact\\ContactServiceProvider"
            ]
        }
    },
    
    "scripts": {
        "post-autoload-dump": [
            "@php artisan migrate --path=vendor/exiliensoft/contact/src/database/migrations --force"
        ],
        "pre-uninstall-cmd": [
            "@php artisan migrate:rollback --path=vendor/exiliensoft/contact/src/database/migrations --force"
        ]
    }


}